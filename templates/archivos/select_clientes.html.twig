          <div class="card offset-lg-3 col-xl-6 col-lg-5 col-md-4 col-sm-12 paddin-top">
            <p>
                <b>Selecciona un cliente </b>
            </p>
            <div class="form-group">
                <select name="clienteid" id="clienteid" class="col-12" id="sel2">
                {% for cliente in clientes %}
