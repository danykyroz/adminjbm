{% extends 'base.html.twig' %}

{% block title %}Flotillas{% endblock %}

{% block body %}
    <h1>Flotillas</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ flotilla.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ flotilla.nombre }}</td>
            </tr>
            <tr>
                <th>Documento</th>
                <td>{{ flotilla.documento }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ flotilla.createdAt ? flotilla.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ flotilla.updatedAt ? flotilla.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

  
    <a class="btn btn-default" href="{{ path('flotillas_edit', {'id': flotilla.id}) }}">Editar</a>
      <a class="btn" href="{{ path('flotillas_index') }}">Volver a la lista</a>

    <br/><br/>
    {{ include('flotillas/_delete_form.html.twig') }}
{% endblock %}
