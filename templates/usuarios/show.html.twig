{% extends 'base.html.twig' %}

{% block title %}FosUser{% endblock %}

{% block body %}
    <h1>FosUser</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fos_user.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ fos_user.username }}</td>
            </tr>
            <tr>
                <th>UsernameCanonical</th>
                <td>{{ fos_user.usernameCanonical }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ fos_user.email }}</td>
            </tr>
            <tr>
                <th>EmailCanonical</th>
                <td>{{ fos_user.emailCanonical }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ fos_user.enabled ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Salt</th>
                <td>{{ fos_user.salt }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ fos_user.password }}</td>
            </tr>
            <tr>
                <th>LastLogin</th>
                <td>{{ fos_user.lastLogin ? fos_user.lastLogin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ConfirmationToken</th>
                <td>{{ fos_user.confirmationToken }}</td>
            </tr>
            <tr>
                <th>PasswordRequestedAt</th>
                <td>{{ fos_user.passwordRequestedAt ? fos_user.passwordRequestedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ fos_user.roles ? fos_user.roles|join(', ') : '' }}</td>
            </tr>
            <tr>
                <th>Locked</th>
                <td>{{ fos_user.locked }}</td>
            </tr>
            <tr>
                <th>Expired</th>
                <td>{{ fos_user.expired }}</td>
            </tr>
            <tr>
                <th>ExpiresAt</th>
                <td>{{ fos_user.expiresAt ? fos_user.expiresAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CredentialsExpireAt</th>
                <td>{{ fos_user.credentialsExpireAt ? fos_user.credentialsExpireAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CredentialsExpired</th>
                <td>{{ fos_user.credentialsExpired }}</td>
            </tr>
            <tr>
                <th>TipoUsuario</th>
                <td>{{ fos_user.tipoUsuario }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('fos_user_index') }}">back to list</a>

    <a href="{{ path('fos_user_edit', {'id': fos_user.id}) }}">edit</a>

    {{ include('fos_user/_delete_form.html.twig') }}
{% endblock %}
